name = WEP Shared/Restricted
author = Dan O'Reilly
version = 2
require key *Key
optional key_index *Key_Index
protected key *Key
-----
ctrl_interface=/var/run/wpa_supplicant
network={
       ssid="$_ESSID"
       scan_ssid=$_SCAN
       key_mgmt=NONE
       auth_alg=SHARED
       wep_key$_KEY_INDEX=$_KEY
       wep_tx_keyidx=$_KEY_INDEX
       priority=5
}
