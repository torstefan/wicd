From: David Paleino <dapal@debian.org>
Subject: remove PYTHONPATHs setting

---
 in/scripts=wicd-cli.in    |    1 +
 in/scripts=wicd-client.in |    3 ---
 in/scripts=wicd-gtk.in    |    3 ---
 in/scripts=wicd.in        |    3 ---
 4 files changed, 1 insertion(+), 9 deletions(-)

--- wicd.orig/in/scripts=wicd-cli.in
+++ wicd/in/scripts=wicd-cli.in
@@ -1,2 +1,3 @@
 #!/bin/bash
+
 exec %PYTHON% -O %SHARE%cli/wicd-cli.py $@
--- wicd.orig/in/scripts=wicd-client.in
+++ wicd/in/scripts=wicd-client.in
@@ -3,9 +3,6 @@ BOLD=$(tput bold)
 BLUE=$(tput setaf 4)
 NC=$(tput sgr0)
 
-PYTHONPATH=%LIB%
-export PYTHONPATH
-
 # check_firstrun()
 if [ ! -d "$HOME/.wicd" ]; then
     mkdir -p "$HOME/.wicd"
--- wicd.orig/in/scripts=wicd-gtk.in
+++ wicd/in/scripts=wicd-gtk.in
@@ -9,7 +9,4 @@ if [ -e "/var/lib/wicd/WHEREAREMYFILES" 
     ln -s "/var/lib/wicd/WHEREAREMYFILES" "$HOME/.wicd/WHEREAREMYFILES"
 fi
 
-PYTHONPATH=%LIB%
-export PYTHONPATH
-
 exec %PYTHON% -O %SHARE%gtk/wicd-client.py $@
--- wicd.orig/in/scripts=wicd.in
+++ wicd/in/scripts=wicd.in
@@ -1,6 +1,3 @@
 #!/bin/sh
 
-PYTHONPATH=%SHARE%
-export PYTHONPATH
-
 exec %PYTHON% -O %SHARE%daemon/wicd-daemon.py $@
